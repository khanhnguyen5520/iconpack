<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/sephiroth_checkbox3_checked_disabled" app:state_indeterminate="false" />
    <item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/sephiroth_checkbox3_indeterminate_disabled" app:state_indeterminate="true" />
    <item android:state_enabled="false" android:state_checked="false" android:drawable="@drawable/sephiroth_checkbox3_indeterminate_disabled" app:state_indeterminate="true" />
    <item android:state_enabled="false" android:state_checked="false" android:drawable="@drawable/sephiroth_checkbox3_unchecked" app:state_indeterminate="false" />
    <item android:state_enabled="true" android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/sephiroth_checkbox3_checked" app:state_indeterminate="false" />
    <item android:id="@id/indeterminate" android:drawable="@drawable/sephiroth_checkbox3_indeterminate" app:state_indeterminate="true" />
    <item android:state_checked="false" android:id="@id/unchecked" android:drawable="@drawable/sephiroth_checkbox3_unchecked" app:state_indeterminate="false" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_checked_to_indeterminate" android:toId="@id/indeterminate" android:fromId="@id/checked" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_indeterminate_to_unchecked" android:toId="@id/unchecked" android:fromId="@id/indeterminate" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_unchecked_to_indeterminate" android:toId="@id/indeterminate" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_indeterminate_to_checked" android:toId="@id/checked" android:fromId="@id/indeterminate" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_unchecked_to_checked" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/sephiroth_checkbox3_transition_checked_to_unchecked" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>